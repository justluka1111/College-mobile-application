<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SixWeekscreen">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="257dp"
        android:layout_height="169dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/btnchild"
        android:layout_width="212dp"
        android:layout_height="58dp"
        android:layout_marginStart="102dp"
        android:layout_marginBottom="29dp"
        android:background="#FF5722"
        android:shadowColor="#000000"
        android:text="CHILD MINDING"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btncook"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#000000"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/btncook"
        android:layout_width="210dp"
        android:layout_height="52dp"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="28dp"
        android:background="#FF5722"
        android:shadowColor="#000000"
        android:text="COOKING"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btngarden"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#000000"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/btngarden"
        android:layout_width="217dp"
        android:layout_height="61dp"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="175dp"
        android:background="#FF5722"
        android:shadowColor="#000000"
        android:text="GARDEN MAINTENANCE"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#000000"
        app:strokeColor="#000000" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="245dp"
        android:layout_height="42dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="58dp"
        android:text="SIX-WEEKS COURSES"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />
</androidx.constraintlayout.widget.ConstraintLayout>